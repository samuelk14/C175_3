<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    tools:context="proyecto2.app1.com.example.pc.c175.TempFragment">

    <TextView
        android:id="@+id/textView"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="COOLANT PUMP UOTLET"
        android:textSize="10sp"
        android:textStyle="bold"
        android:textColor="#ffc400"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="30dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="ENGINE COLLANT TEMP."
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="30dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="AMBIENT AIR TEMP."
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintTop_toBottomOf="@+id/seekBar3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="INTAKE MANIFOLD AIR TEMP. #1"
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintLeft_toRightOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/seekBar2"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="ENGINE OIL TEMP."
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="6dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="30dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="FUEL TEMP."
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="30dp" />

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:max="10"
        android:progress="3"
        android:rotation="270"
        app:layout_constraintLeft_toRightOf="@+id/seekBar3"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="37dp" />

    <SeekBar
        android:id="@+id/seekBar3"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:max="10"
        android:progress="3"
        android:rotation="270"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="37dp" />

    <SeekBar
        android:id="@+id/seekBar4"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:max="10"
        android:progress="3"
        android:rotation="270"
        app:layout_constraintLeft_toRightOf="@+id/seekBar2"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="37dp" />

    <SeekBar
        android:id="@+id/seekBar5"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:max="10"
        android:progress="3"
        android:rotation="270"
        app:layout_constraintLeft_toRightOf="@+id/seekBar4"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="37dp" />

    <SeekBar
        android:id="@+id/seekBar6"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:max="10"
        android:progress="3"
        android:rotation="270"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <SeekBar
        android:id="@+id/seekBar7"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:max="10"
        android:progress="3"
        android:rotation="270"
        app:layout_constraintLeft_toRightOf="@+id/seekBar6"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TEMPERATURES"
        android:textStyle="bold"
        android:textColor="#ffc400"
        android:textSize="15dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.331"
        android:layout_marginTop="-1dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="INTAKE MANIFOLD AIR TEMP. #2"
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintTop_toBottomOf="@+id/seekBar4"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="40dp" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:max="10"
        android:progress="3"
        android:rotation="270"
        android:background="#212121"
        app:layout_constraintLeft_toRightOf="@+id/seekBar7"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <SeekBar
        android:id="@+id/seekBar8"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:max="10"
        android:progress="3"
        android:background="#212121"
        android:rotation="270"
        app:layout_constraintLeft_toRightOf="@+id/seekBar"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="LEFT EXHAUST TEMP."
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintTop_toBottomOf="@+id/seekBar5"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="40dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="FUEL RAIL TEMP."
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView6"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="30dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="75dp"
        android:layout_height="35dp"
        android:text="RIGHT EXHAUST TEMP."
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:textColor="#ffc400"
        app:layout_constraintTop_toBottomOf="@+id/seekBar9"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="40dp" />

    <SeekBar
        android:id="@+id/seekBar9"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:rotation="270"
        android:max="10"
        android:progress="3"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintLeft_toRightOf="@+id/seekBar5"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="37dp" />

    <SeekBar
        android:id="@+id/seekBar10"
        android:layout_width="80dp"
        android:layout_height="18dp"
        android:background="#212121"
        android:rotation="270"
        android:max="10"
        android:progress="3"
        app:layout_constraintLeft_toRightOf="@+id/seekBar8"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

</android.support.constraint.ConstraintLayout>
